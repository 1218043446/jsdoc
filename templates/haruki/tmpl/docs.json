{
	"classes":{
		{:reduce classes}
		"{=name}": {
			"name": "{=name}",
			"properties":{{:reduce properties}
				"{=name}": {
					"name": "{=name}",
					"description": "{=json desc}"
				}{:if $last}{:else},{/:if}
			{/:reduce properties}},
			"methods":{{:reduce methods}
				"{=name}": {
					"name": "{=name}",
					"description": "{=json desc}"
				}{:if $last}{:else},{/:if}
			{/:reduce methods}},
			"description": "{=json desc}"
		}
		{/:reduce classes}
	}
}
